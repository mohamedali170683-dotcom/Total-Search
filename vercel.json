{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/home"
    },
    {
      "src": "^/home$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/brand$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/demand$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/compare$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/debug$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/test-template$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/test-simple-dashboard$",
      "dest": "/api/main.py"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/main.py"
    }
  ]
}
