{
  "version": 2,
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/main.py"
    }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@4.5.1",
      "maxDuration": 60
    }
  }
}
